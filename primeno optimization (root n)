#include <iostream>

using namespace std;

bool primeno(int n)
{
    
    if (n<=1){
        return false;
    }
    for (int i=2;i*i<n;i++){
        if(n%i==0){
         return false;
    } 
    }    
return true;
}
int main()
{
    int n;
    cin>>n;
    if (primeno(n)==true)
    {
        cout<<" is prime"<<endl;
    }
    else{
    
     cout<<"not prime "<<endl;
     
    }
return 0;
    
}

